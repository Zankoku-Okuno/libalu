// DEPS: <stddef.h> <stdint.h> <stdbool.h> "addF_@T.h" "subF_@T.h" "divmod_@T.h" "divmodF_@T.h"

inline
@TYPE align_uptoF_@T(@TYPE a, @TYPE to, bool* sticky_bit) {
    @TYPE tmp = modF_@T(a, to, sticky_bit);
    if (tmp == 0) {
        return a;
    }
    else {
        @TYPE adjust = subF_@T(to, tmp, sticky_bit); // TODO is it necessary to use checked arith here?
        return addF_@T(a, adjust, sticky_bit);
    }
}

inline
@TYPE align_backtoF_@T(@TYPE a, @TYPE to, bool* sticky_bit) {
    @TYPE adjust = modF_@T(a, to, sticky_bit);
    return subF_@T(a, adjust, sticky_bit); // TODO is it necessary to use checked arith here?
}
